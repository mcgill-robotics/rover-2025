{
  "version": 8,
  "name": "Satellite - Desert Optimized",
  "sources": {
    "satellite": {
      "type": "raster",
      "tiles": ["http://localhost:8080/styles/satellite/{z}/{x}/{y}.png"],
      "tileSize": 256
    }
  },
  "layers": [
    {
      "id": "satellite",
      "type": "raster",
      "source": "satellite",
      "minzoom": 0,
      "maxzoom": 22,
      "paint": {
        "raster-opacity": 0.95,
        "raster-contrast": 1.1
      }
    }
  ]
}
